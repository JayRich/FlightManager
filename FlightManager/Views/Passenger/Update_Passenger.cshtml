@model Passenger


@{

}

<div class="display-4 text-center">Update Passenger Info.</div>



<div class="row">
    <div class="col-md-4">
        <form asp-controller="Passenger" asp-action="Find_Update_Passenger">

            <div class="form-group">
                <label for="id" class="control-label">Passenger Id</label>
                <input type="text" name="id" class="form-control" />
            </div>
            <div class="form-group">
                <label for="n" class="control-label">Name</label>
                <input type="text" name="n" class="form-control" />

            </div>
            <div class="form-group">
                <label for="j" class="control-label">Job</label>
                <input type="text" name="j" class="form-control" />

            </div>
            <div class="form-group">
                <label for="e" class="control-label">Email</label>
                <input type="text" name="e" class="form-control" />

            </div>
            <div class="form-group">
                <label for="a" class="control-label">Age</label>
                <input type="text" name="a" class="form-control" />

            </div>
            <div class="form-group">
                <label for="fn" class="control-label">Flight Number</label>
                <input type="text" name="fn" class="form-control" />

            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-info">Submit</button>
            </div>
        </form>

    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
